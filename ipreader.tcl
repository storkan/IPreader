#main program
maingui

if {[llength $argv]==1} {
  set gui(filename) [lindex $argv 0]
  readimage
} elseif {[llength $argv]==0} {
  set gui(filename) ""
} else {
  puts "\n\nsyntax: ipreader filename"
  puts "filename   = filename of a png graphic"
  exit 1
}
